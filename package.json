{
  "name": "videostil",
  "version": "0.2.3",
  "description": "Convert videos into LLM-friendly input by extracting and deduplicating frames",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "videostil": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && cd viewer && npm install && npm run build && cd ..",
    "build:viewer": "cd viewer && npm run build && cd ..",
    "dev": "nodemon",
    "dev:viewer": "cd viewer && npm run dev && cd ..",
    "dev:ts": "tsc --watch",
    "prepare": "npm run build && husky",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write . --unsafe",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "video",
    "frames",
    "llm",
    "deduplication",
    "ffmpeg",
    "computer-vision"
  ],
  "author": "Empirical",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empirical-run/videostil.git"
  },
  "bugs": {
    "url": "https://github.com/empirical-run/videostil/issues"
  },
  "homepage": "https://github.com/empirical-run/videostil#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@empiricalrun/llm": "^0.24.0",
    "commander": "^14.0.1",
    "open": "^10.0.0",
    "pixelmatch": "^6.0.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^20.0.0",
    "@types/pixelmatch": "^5.2.6",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
